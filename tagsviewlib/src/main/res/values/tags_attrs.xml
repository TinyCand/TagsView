<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title"  format="reference|string"/>
    <attr name="verticalSpacing" format="reference|dimension"/>
    <attr name="horizontalSpacing" format="reference|dimension"/>

    <attr name="stretchMode" format="reference|dimension">
        <enum name="none" value="0"/>
        <enum name="spacingWidth" value="1"/>
        <enum name="spacingWidth_auto" value="5"/>
    </attr>

    <declare-styleable name="TagsView">
        <attr name="verticalSpacing"/>
        <attr name="horizontalSpacing" />
        <attr name="stretchMode" />
    </declare-styleable>
</resources>
